package libro.Tema1;

public class Ejercicio_7 {

	public static void main(String[] args) {
		String punto = "come-pollas";
		System.out.println("    " + punto);
		System.out.println("   " + punto + " " + punto);
		System.out.println("  " + punto + "   " + punto);
		System.out.println(" " + punto + "     " + punto);
		System.out.println(punto + "       " + punto);
	}

}
