		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int num1 = 4;
		long num2 = 4;
		double num3 = 4;
		float num4 = 4;
		System.out.printf("%d %d %f %f", num1, num2, num3, num4);
