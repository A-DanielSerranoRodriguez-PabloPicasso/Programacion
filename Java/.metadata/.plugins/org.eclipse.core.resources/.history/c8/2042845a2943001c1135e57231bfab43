package utils;

public class Comprobar {
	public static boolean entero(String n) {
		boolean entero;
		for (int i = 0; i < n.toCharArray().length; i++) {
			if (n.toCharArray()[i] == '.' || n.toCharArray()[i] == ',' || Character.isLetter(n.toCharArray()[i])) {
				return entero = false;
			}
		}
		return entero = true;
	}
}
